<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".blogHesabimiz">


    <LinearLayout
        android:gravity="center"
        android:layout_marginTop="50dp"
        android:layout_width="410dp"
        android:layout_height="720dp">

        <WebView
            android:id="@+id/webView1"
            android:layout_width="410dp"
            android:layout_height="720dp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="1dp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/kepViewBlogHesabimiz"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="330dp"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="2dp"
        android:layout_marginEnd="26dp"
        app:srcCompat="@drawable/kep"
        />

    <ImageView
        android:id="@+id/geriGitBlogHesabimiz"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_marginStart="1dp"
        android:onClick="geriGitBlogHesabimiz"
        android:layout_marginTop="5dp"
        app:srcCompat="@drawable/geri3" />
    <TextView
        android:id="@+id/ogrencininMailiBlogHesabimiz"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        />

    <TextView
        android:id="@+id/ogrencininBolumuBlogHesabimiz"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginStart="170dp"
        android:layout_marginTop="10dp"
        />
</RelativeLayout>
